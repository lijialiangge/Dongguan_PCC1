@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width,height=device-height,inital-scale=1.0,maximum-scale=1.0,user-scalable=no;" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="远程通讯系统keywords">
    <meta name="description" content="远程通讯系统description">
    <title>History</title>

    <link rel="stylesheet" href="~/Css/layui/modules/layui/css/layui.css"/>
    <link rel="stylesheet" href="~/Css/layui/css/admin.css" />
    <script src="~/Css/layui/js/jquery-1.12.4.min.js"></script>
</head>
<body>
    


    <div class="iframe-main">

        <!--报表相关数据-->
        <div class="layui-content-nav layui-block">
            <div class="layui-row layui-content-nav-flush">
                <div class="layui-col-sm12 layui-content-nav-tab">
                    <div class="layui-tab layui-tab-brief">
                        <ul class="layui-tab-title layui-tab-nav">
                            <li class="layui-this">OEE</li>
                            <li>PO</li>
                            <li>Daily Output</li>
                            <li>Monthly Output</li>
                            <li>Cap</li>
                            <li>ESR</li>
                            <li>Voltage</li>
                        </ul>
                        <div class="layui-tab-content">
                            @*OEE*@
                            <div class="layui-tab-item layui-show temperature-fomdata content-table">
                                <div class="layui-form layui-table-form-list">
                                    <form class="layui-form">
                                        <label class="layui-form-label">Line/Page</label>
                                        <div class="layui-input-inline">
                                            <select name="limit" id="warning-input-limit">
                                                <option value="10" selected="">10</option>
                                                <option value="20">20</option>
                                                <option value="30">30</option>
                                                <option value="50">50</option>
                                            </select>
                                        </div>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime start_time" id="dateTime1" name="start_time">
                                            <input type="text" class="layui-input layui-dateTime DeviceId" value="@ViewBag.DeviceId" id="DeviceId" name="DeviceId" style="display:none">
                                        </div>
                                        <label>To</label>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime end_time" id="dateTime2" name="end_time">
                                        </div>
                                        <button type="button" class="layui-btn layui-btn-radius layui-btn-sm" lay-submit="" lay-filter="search-oee-fomdata">Search</button>
                                        <div class="layui-inline layui-inline-right" id="layui-table-tool">
                                        </div>
                                    </form>
                                </div>

                                <table class="layui-table" id="oee-fomdata-list"></table>
                            </div>
                            @*OEE*@

                            @*PO*@
                            <div class="layui-tab-item technique-fomdata content-table">
                                <div class="layui-form layui-table-form-list">
                                    <form class="layui-form">
                                        <label class="layui-form-label">Line/Page</label>
                                        <div class="layui-input-inline">
                                            <select name="limit" id="warning-input-limit">
                                                <option value="10" selected="">10</option>
                                                <option value="20">20</option>
                                                <option value="30">30</option>
                                                <option value="50">50</option>
                                            </select>
                                        </div>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime start_time" id="dateTime3" name="start_time">
                                            <input type="text" class="layui-input layui-dateTime DeviceId" value="@ViewBag.DeviceId" id="DeviceId" name="DeviceId" style="display:none">
                                        </div>
                                        <label>To</label>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime end_time" id="dateTime4" name="end_time">
                                        </div>
                                        <button type="button" class="layui-btn layui-btn-radius layui-btn-sm" lay-submit="" lay-filter="search-po-fomdata">Search</button>
                                        <div class="layui-inline layui-inline-right" id="layui-table-tool">
                                        </div>
                                    </form>
                                </div>
                                <table class="layui-table" id="po-fomdata-list"></table>
                            </div>
                            @*PO*@

                            @*Daily Output*@
                            <div class="layui-tab-item others-fomdata content-table">
                                <div class="layui-form layui-table-form-list">
                                    <form class="layui-form">
                                        <label class="layui-form-label">Line/Page</label>
                                        <div class="layui-input-inline">
                                            <select name="limit" id="warning-input-limit">
                                                <option value="10" selected="">10</option>
                                                <option value="20">20</option>
                                                <option value="30">30</option>
                                                <option value="50">50</option>
                                            </select>
                                        </div>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime start_time" id="dateTime5" name="start_time">
                                            <input type="text" class="layui-input layui-dateTime DeviceId" value="@ViewBag.DeviceId" id="DeviceId" name="DeviceId" style="display:none">
                                        </div>
                                        <label>To</label>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime end_time" id="dateTime6" name="end_time">
                                        </div>
                                        <button type="button" class="layui-btn layui-btn-radius layui-btn-sm" lay-submit="" lay-filter="search-daily-ouput-fomdata">Search</button>
                                        <div class="layui-inline layui-inline-right" id="layui-table-tool">
                                        </div>
                                    </form>
                                </div>
                                <table class="layui-table" id="daily-ouput-fomdata-list"></table>
                            </div>
                            @*Daily Output*@

                            @*Monthly Output*@
                            <div class="layui-tab-item others-fomdata content-table">
                                <div class="layui-form layui-table-form-list">
                                    <form class="layui-form">
                                        <label class="layui-form-label">Line/Page</label>
                                        <div class="layui-input-inline">
                                            <select name="limit" id="warning-input-limit">
                                                <option value="10" selected="">10</option>
                                                <option value="20">20</option>
                                                <option value="30">30</option>
                                                <option value="50">50</option>
                                            </select>
                                        </div>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime start_time" id="dateTime7" name="start_time">
                                            <input type="text" class="layui-input layui-dateTime DeviceId" value="@ViewBag.DeviceId" id="DeviceId" name="DeviceId" style="display:none">
                                        </div>
                                        <label>To</label>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime end_time" id="dateTime8" name="end_time">
                                        </div>
                                        <button type="button" class="layui-btn layui-btn-radius layui-btn-sm" lay-submit="" lay-filter="search-monthlyouput-fomdata">Search</button>
                                        <div class="layui-inline layui-inline-right" id="layui-table-tool">
                                        </div>
                                    </form>
                                </div>
                                <table class="layui-table" id="monthly-ouput-fomdata-list"></table>
                            </div>
                            @*Monthly Output*@

                            @*Cap*@
                            <div class="layui-tab-item others-fomdata content-table">
                                <div class="layui-form layui-table-form-list">
                                    <form class="layui-form">
                                        <label class="layui-form-label">Line/Page</label>
                                        <div class="layui-input-inline">
                                            <select name="limit" id="warning-input-limit">
                                                <option value="10" selected="">10</option>
                                                <option value="20">20</option>
                                                <option value="30">30</option>
                                                <option value="50">50</option>
                                            </select>
                                        </div>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime start_time" id="dateTime7" name="start_time">
                                            <input type="text" class="layui-input layui-dateTime DeviceId" value="@ViewBag.DeviceId" id="DeviceId" name="DeviceId" style="display:none">
                                        </div>
                                        <label>To</label>
                                        <div class="layui-inline">
                                            <input type="text" class="layui-input layui-dateTime end_time" id="dateTime8" name="end_time">
                                        </div>
                                        <button type="button" class="layui-btn layui-btn-radius layui-btn-sm" lay-submit="" lay-filter="search-monthlyouput-fomdata">Search</button>
                                        <div class="layui-inline layui-inline-right" id="layui-table-tool">
                                        </div>
                                    </form>
                                </div>
                                <table class="layui-table" id="monthly-ouput-fomdata-list"></table>
                            </div>
                            @*Cap*@
                        </div>
                    </div>
                </div>

                <div class="layui-content-nav-right">
                    <div class="layui-btn-right" style="width:346px;">
                        <button onclick="window.location.href='@Url.Action(ViewBag.DeviceId, "PCTron", new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">Main</button>
                        <button onclick="window.location.href='@Url.Action("History", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary layui-btn-this">History</button>
                        <button onclick="window.location.href='@Url.Action("Alarm", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">Alarm</button>
                        <button onclick="window.open('@Url.Action("BigScreen", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })')" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">BigScreen</button>
                    </div>
                </div>
            </div>
                </div>
                <!--报表画面相关结束-->


            </div>



    <script src="~/Css/layui/modules/layui/layui.all.js"></script>
    <script src="~/Scripts/layui/globs.js"></script>
</body>

</html>
