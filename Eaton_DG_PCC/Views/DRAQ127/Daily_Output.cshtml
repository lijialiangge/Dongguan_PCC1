@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Daily_Output</title>

    <link rel="stylesheet" href="~/Css/layui/layui.css" />
    <link rel="stylesheet" href="~/Css/layui/admin.css" />
</head>
<body class="gray-bg">
   
    <div>
        Daily_Output
    </div>
    <div class="row">
        <div class="col-sm-4">
            <div class="ibox2 float-e-margins">
                <div class="ibox-title">
                    <div class="row">
                        <div class="layui-btn-right">
                            <button onclick="window.location.href='@Url.Action(ViewBag.DeviceId, "PCTron", new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">Main</button>
                            <button onclick="window.location.href='@Url.Action("OEE", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">OEE</button>
                            <button onclick="window.location.href='@Url.Action("Order", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">Order</button>
                            <button onclick="window.location.href='@Url.Action("Daily_Output", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary layui-btn-this">Daily Output</button>
                            <button onclick="window.location.href='@Url.Action("Monthly_Output", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })'" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">Monthly Output</button>
                            <button onclick="window.open('@Url.Action("BigScreen", ViewBag.DeviceId, new { DeviceId = ViewBag.DeviceId })')" type="button" class="layui-btn layui-btn-radius layui-btn-sm layui-btn-primary">BigScreen</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
   

</body>

</html>
