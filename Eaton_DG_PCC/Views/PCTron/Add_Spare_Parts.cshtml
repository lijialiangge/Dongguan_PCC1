@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add Faq</title>
    <link href="~/Css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="~/Css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <link href="~/Css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <link href="~/Css/plugins/sweetalert/sweetalert.css" rel="stylesheet">

</head>
<body class="gray-bg">
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>


    <div class="wrapper wrapper-content animated fadeInRight">


        <div class="row">
            <div class="col-sm-8">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Add</h5>
                    </div>
                    <div class="ibox-content">
                        <form class="form-horizontal m-t" id="signupForm" action="@Url.Action("Add_Spare_Parts")" method="post">

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Address：</label>
                                <div class="col-sm-8">
                                    <input id="name" readonly="readonly"   name="add_spare_parts" value="@ViewBag.Address" />
                                    @*<input id="name" name="add_spare_parts" type="hidden" value="@ViewBag.Address" />*@
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Number：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>


                            <div class="form-group">
                                <label class="col-sm-3 control-label">Brand：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label">Model：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />

                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Number of Stock：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Unit：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>


                            <div class="form-group">
                                <label class="col-sm-3 control-label">Safety Stock：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Provider：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label">Owner：</label>
                                <div class="col-sm-8">
                                    <input id="name" name="add_spare_parts" type="text" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label">Telephone：</label>
                                <div class="col-sm-8">
                                    <textarea cols="111" name="add_spare_parts" rows="2"></textarea>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">Make：</label>
                                <div class="col-sm-8">
                                    <textarea cols="111" name="add_spare_parts" rows="2"></textarea>
                                </div>
                            </div>




                            <div class="form-group">
                                <div class="col-sm-8 col-sm-offset-3">
                                    <button class="btn btn-primary saveStatus" id="saving" type="submit">Save</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>




    </div>
    <script src="~/scripts/jquery.min.js?v=2.1.4"></script>
    <script src="~/scripts/plugins/sweetalert/sweetalert.min.js"></script>
</body>

</html>
